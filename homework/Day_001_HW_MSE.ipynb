   "source": [
    "def HW1_MSE(Y,Y_hat):\n",
    "    n_max=len(Y)\n",
    "    Temp_M=0\n",
    "    for n in range(0,n_max):\n",
    "        T=Y[n]-Y_hat[n]\n",
    "        Temp_M=T*T+Temp_M\n",
    "\n",
    "    Temp_M=Temp_M/n_max\n",
    "    print('MSE=',Temp_M)"
   ]
  }
